@{
    string heading = "Welcome!";
    List<string> stuffToDo = new List<string>
    {
        "Click",
        "Close",
        "Refresh"
    };
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Index</title>
</head>
<body>
    <h1>@heading</h1><hr />
    <h2>Stuff to do:</h2>
    @foreach (var item in stuffToDo)
    {
        <li>You can @item!</li>
    }
</body>
</html>
